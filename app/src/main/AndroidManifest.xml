<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ginsmile.pokemaster">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AllAppActivity"
            android:label="@string/title_activity_activity_all_apps"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".RunningActivity"
            android:label="@string/title_activity_running"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".SystemInfoActivity"
            android:label="@string/title_activity_system_info"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".FileActivity"
            android:label="@string/title_activity_file"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
